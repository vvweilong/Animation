<?xml version="1.0" encoding="utf-8"?>
<com.oo.valueanimation.PullToRefreshLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimaryDark"
    tools:context="com.oo.valueanimation.ValueAnimationActivity">


    <Button
        android:id="@+id/target"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />


    <com.oo.valueanimation.ColumView
        android:id="@+id/colum_view"
        android:layout_width="30dp"
        android:layout_height="match_parent" />


</com.oo.valueanimation.PullToRefreshLayout>
